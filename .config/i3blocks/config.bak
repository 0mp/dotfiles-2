[dlna]
full_text=  
command=[[ -z "${BLOCK_BUTTON}" ]] || ( [[ "${BLOCK_BUTTON}" == "1" ]] && ((/usr/bin/pavucontrol -t 3 || true) && $HOME/.bin/choose_audio_output.sh) || sleep 1 ) ; pgrep pulseaudio-dlna &> /dev/null && echo "  " || echo
interval=5
signal=1
border=#e5e5e5
#border=#917033

[volume]
#command=[[ -z "${BLOCK_BUTTON}" ]] || ( [[ "${BLOCK_BUTTON}" == "1" ]] && ((/usr/bin/pavucontrol -t 3 || true) && $HOME/scripts/volume_control.py signal) || $HOME/scripts/volume_control.py toggle ); $HOME/scripts/volume_control.py i3blocks
